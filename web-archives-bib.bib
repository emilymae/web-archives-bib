  pages = {321-330},
  file = {/Users/EM/Box Sync/00-iSchool - Current/00-Web Archives/Brunelle et al_2014_Not all mementos are created equal.pdf;/Users/EM/Zotero/storage/NUEWSFNE/Brunelle et al. - 2014 - Not all mementos are created equal Measuring the .pdf;/Users/EM/Zotero/storage/UFV224E2/6970187.html}
