@article{Stevensoncyberculturalmomentnew2016,
  title = {The Cybercultural Moment and the New Media Field},
  volume = {18},
  issn = {1461-4448},
  doi = {10.1177/1461444816643789},
  abstract = {This article draws on Pierre Bourdieu's field theory to understand the regenerative ``belief in the new'' in new media culture and web history. I begin by noting that discursive constructions of the web as disruptive, open, and participatory have emerged at various points in the medium's history, and that these discourses are not as neatly tied to economic interests as most new media criticism would suggest. With this in mind, field theory is introduced as a potential framework for understanding this (re)production of a belief in the new as a dynamic of the interplay of cultural and symbolic forms of capital within the new media field. After discussing how Bourdieu's theory might be applied to new media culture in general terms, I turn to a key moment in the emergence of the new media field\textemdash{}the rise of cybercultural magazines Mondo 2000 and Wired in the early 1990s\textemdash{}to illustrate how Bourdieu's theory may be adapted in the study of new media history.},
  language = {en},
  number = {7},
  journal = {New Media \& Society},
  author = {Stevenson, Michael},
  month = aug,
  year = {2016},
